<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_pressed="true" >
		<shape>
			<gradient 
				android:startColor="#292929" 
				android:endColor="#292929" 
				android:angle="270" />

			<stroke 
				android:width="1dp" 
				android:color="#E3E3E3" />
			<corners 
				android:radius="2dp" />

		</shape>
	</item>
	<item android:state_pressed="false" >
		<shape>
			<gradient 
				android:startColor="#292929" 
				android:endColor="#292929" 
				android:angle="270" />
			<stroke 
				android:width="0dp"
				android:color="#292929" />
			<corners 
				android:radius="2dp" />
		</shape>
	</item>
</selector>
